    <div class="panel panel-primary">
      <div class="panel-heading">
        <h5 class="panel-title text-center">[% infoBoxTitle %]</h5>
      </div>
    <div class="panel-body">    
    
    <div class=row>
    <div class=col-xs-4>
      <div class="panel panel-default">
        <div class="panel-heading">
          <h5 class="panel-title text-center">Current State</h5>
        </div>
        <div class="panel-body">
        [% PROCESS _infobox.tt %]
        </div>
      </div>
    </div>
    <div class=col-xs-4>
      <div class="panel panel-default">
        <div class="panel-heading">
          <h5 class="panel-title text-center">Monitoring Performance</h5>
        </div>
        <div class="panel-body">
          <table border="0" cellspacing="4" class="perfTitleTable">
            <tr>
              <td>
                <table border="0" cellspacing="0" class="perfBox">
                  <tr>
                    <td class='perfBox'>
                      <table border="0" cellspacing="4">
                        <tr>
                          <td align="left" valign="middle" class='perfItem'><a href='extinfo.cgi?type=4' class='perfItem'>Service Check Execution Time:</a></td>
                          <td valign="top" class='perfValue' nowrap><a href='extinfo.cgi?type=4' class='perfValue'>[% sprintf('%.2f',stats.services_execution_time_min) %] / [% sprintf('%.2f',stats.services_execution_time_max) %] / [% sprintf('%.3f',stats.services_execution_time_avg) %] sec</a></td>
                        </tr>
                        <tr>
                          <td align="left" valign="middle" class='perfItem'><a href='extinfo.cgi?type=4' class='perfItem'>Service Check Latency:</a></td>
                          <td valign="top" class='perfValue' nowrap><a href='extinfo.cgi?type=4' class='perfValue'>[% sprintf('%.2f',stats.services_latency_min) %] / [% sprintf('%.2f',stats.services_latency_max) %] / [% sprintf('%.3f',stats.services_latency_avg) %] sec</a></td>
                        </tr>
                        <tr>
                          <td align="left" valign="middle" class='perfItem'><a href='extinfo.cgi?type=4' class='perfItem'>Host Check Execution Time:</a></td>
                          <td valign="top" class='perfValue' nowrap><a href='extinfo.cgi?type=4' class='perfValue'>[% sprintf('%.2f',stats.hosts_execution_time_min) %] / [% sprintf('%.2f',stats.hosts_execution_time_max) %] / [% sprintf('%.3f',stats.hosts_execution_time_avg) %] sec</a></td>
                        </tr>
                        <tr>
                          <td align="left" valign="middle" class='perfItem'><a href='extinfo.cgi?type=4' class='perfItem'>Host Check Latency:</a></td>
                          <td valign="top" class='perfValue' nowrap><a href='extinfo.cgi?type=4' class='perfValue'>[% sprintf('%.2f',stats.hosts_latency_min) %] / [% sprintf('%.2f',stats.hosts_latency_max) %] / [% sprintf('%.3f',stats.hosts_latency_avg) %] sec</a></td>
                        </tr>
                        <tr>
                          <td align="left" valign="middle" class='perfItem'><a href='status.cgi?host=all&amp;serviceprops=131072' class='perfItem'># Active Host / Service Checks:</a></td>
                          <td valign="top" class='perfValue' nowrap>
                            <a href='status.cgi?hostgroup=all&amp;hostprops=131072&amp;style=hostdetail' class='perfValue'>[% IF !empty %][% host_stats.total_active %][% ELSE %]0[% END %]</a> / <a href='status.cgi?host=all&amp;serviceprops=131072' class='perfValue'>[% IF !empty %][% service_stats.total_active %][% ELSE %]0[% END %]</a>
                          </td>
                        </tr>
                        <tr>
                          <td align="left" valign="middle" class='perfItem'><a href='status.cgi?host=all&amp;serviceprops=65536' class='perfItem'># Passive Host / Service Checks:</a></td>
                          <td valign="top" class='perfValue' nowrap><a href='status.cgi?hostgroup=all&amp;hostprops=65536&amp;style=hostdetail' class='perfValue'>[% IF !empty %][% host_stats.total_passive %][% ELSE %]0[% END %]</a> / <a href='status.cgi?host=all&amp;serviceprops=65536' class='perfValue'>[% IF !empty %][% service_stats.total_passive %][% ELSE %]0[% END %]</a></td>
                        </tr>
                      </table>
                    </td>
                  </tr>
                </table>
              </td>
            </tr>
          </table>

        </div>
      </div>
    </div>

    <div class=col-xs-4>
      <div class="panel panel-default">
        <div class="panel-heading">
          <h5 class="panel-title text-center">Network Health</h5>
        </div>
        <div class="panel-body">    
            <table border="0" cellspacing="4" style=width:100%>
              <tr>
                <td>
                  <table border="0" cellspacing="0" style="width:100%">
                    <tr>
                      <td class='healthBox'>
                        <table border="0" cellspacing="4" style="width:100%">
                          <tr>
                            <td style=width:100px align="left" valign="middle" class='healthItem'>Host Health:&nbsp;</td>
                            [% IF empty || (host_stats.total-host_stats.pending) == 0 %]
                              [% host_health = 0 %]
                            [% ELSE %]
                              [% host_health = host_stats.up / (host_stats.total-host_stats.pending) * 100 %]
                            [% END %]
                            [% health_perc = sprintf('%i', host_health) %]
                            [% IF health_perc >= 90 %]
                            [% host_health_pic = 'success' %]
                            [% ELSIF health_perc >= 75 %]
                            [% host_health_pic = 'warning' %]
                            [% ELSIF health_perc >= 5 %]
                            [% host_health_pic = 'danger' %]
                            [% ELSE %]
                            [% host_health_pic = 'danger' %]
                            [% health_perc = 5 %]
                            [% END %]
                            <!-- td valign="top" width="100" class='healthBar'><div class="health_bar_bg"><img src='[% url_prefix %]themes/[% theme %]/images/[% host_health_pic %]' border="0" width="[% health_perc %]" height="20" alt='[% FILTER format('%2.1f') %][% host_health %][% END %]% Health' title='[% FILTER format('%2.1f') %][% host_health %][% END %]% Health'></div></td -->
                            <td style=width:180px class="progress">
                              <div class="progress-bar progress-bar-[% host_health_pic %]" role="progressbar"
                              aria-valuenow="[% health_perc %]" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width: [% health_perc %]%">
                                [% health_perc %]%
                              </div>
                            </td>
                          </tr>
                          <tr>
                            <td align="left" valign="middle" class='healthItem'>Service Health:</td>
                            [% IF empty || (service_stats.total-service_stats.pending) == 0 %]
                              [% service_health = 0 %]
                            [% ELSE %]
                              [% service_health = (service_stats.ok*2 + service_stats.warning + service_stats.unknown) / ((service_stats.total-service_stats.pending)*2) * 100 %]
                            [% END %]
                            [% service_perc = sprintf('%i', service_health) %]
                            [% IF service_health >= 90 %]
                            [% service_health_pic = 'success' %]
                            [% ELSIF service_health >= 75 %]
                            [% service_health_pic = 'warning' %]
                            [% ELSIF service_health >= 5 %]
                            [% service_health_pic = 'danger' %]
                            [% ELSE %]
                            [% service_health_pic = 'danger' %]
                            [% service_perc = 5 %]
                            [% END %]
                            <!-- td valign="top" width="100" class='healthBar'><div class="health_bar_bg"><img src='[% url_prefix %]themes/[% theme %]/images/[% service_health_pic %]' border="0" width="[% service_perc %]" height="20" alt='[% FILTER format('%2.1f') %][% service_health %][% END %]% Health' title='[% FILTER format('%2.1f') %][% service_health %][% END %]% Health'></div></td -->
                            <td class="progress">
                              <div class="progress-bar progress-bar-[% service_health_pic %]" role="progressbar"
                              aria-valuenow="[% service_perc %]" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width: [% service_perc %]%">
                                [% service_perc %]%
                              </div>
                            </td>
                          </tr>

                          <tr>
                          <tr><td colspan="10" style=padding-top:20px> </td></tr>
                            <td colspan="2" height="20" class='outageTitle'>&nbsp;Network Outages</td>
                          </tr>
                          <tr>
                            <td class='outageHeader' colspan="2" width=""><a href='outages.cgi' class='outageHeader'>[% host_stats.outages %] Outages</a></td>
                          </tr>
                            [% IF !empty && host_stats.outages > 0 %]
                            <tr><td width="100%" colspan="2" class='outageImportantProblem'><a href='outages.cgi'>[% host_stats.outages %] Blocking Outages</a></td></tr>
                            [% END %]
                            
                        </table>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>    
         </div>
      </div>
    </div>   
    </div>    
    </div>
    </div>
 
