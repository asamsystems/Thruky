                      [% IF host_stats.active_checks_disabled_active > 0 %]<tr><td width="100%" class='featureItemDisabledActiveHostChecks'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=131088'>[% host_stats.active_checks_disabled_active %] Hosts Disabled</a></td></tr>[% END %]
                      [% IF host_stats.active_checks_disabled_passive > 0 %]<tr><td width="100%" class='featureItemEnabledActiveHostChecks'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=65552'>[% host_stats.active_checks_disabled_passive %] Hosts Passive</a></td></tr>[% END %]
                    [% ELSE %]
                      <tr><td width="100%" class='featureItemDisabledActiveHostChecks'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=16'>[% host_stats.active_checks_disabled_active + host_stats.active_checks_disabled_passive %] Hosts Disabled</a></td></tr>
                    [% END %]
                  [% END %]
                </table>
              </td>
              [% END %]
            </tr>
          </table>
        </td>
        <td valign="top">
          <table border="0" width="" cellspacing="0" cellpadding="0">
            <tr>
              [% IF pi.keys.size == 0 || pi.accept_passive_service_checks == 1 %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacenabled.png' border='0' alt='Passive Checks Enabled' title='Passive Checks Enabled' width="20" height="76"></a></td>
              [% ELSIF pi.accept_passive_service_checks == -1 %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacdiverse.png' border='0' alt='Passive Checks Ambiguous' title='Passive Checks Ambiguous' width="20" height="76"></a></td>
              [% ELSE %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacdisabled.png' border='0' alt='Passive Checks Disabled' title='Passive Checks Disabled' width="20" height="76"></a></td>
              [% END %]
              <td width="10">&nbsp;</td>
              [% IF pi.keys.size > 0 && pi.accept_passive_service_checks == 0 %]
              <td valign="middle" width="100%" class='featureDisabledPassiveChecks'>N/A</td>
              [% ELSE %]
              <td valign="top" width="100%" class='featureEnabledPassiveChecks'>
                <table border="0" width="100%">
                  <tr>
                    [% IF empty || service_stats.passive_checks_disabled == 0 %]
                    <td width="100%" class='featureItemEnabledPassiveServiceChecks'>All Services Enabled</td>
                    [% ELSE %]
                    <td width="100%" class='featureItemDisabledPassiveServiceChecks'><a href='status.cgi?host=all&amp;type=detail&amp;serviceprops=16384'>[% service_stats.passive_checks_disabled %] Service Disabled</a></td>
                    [% END %]
                  </tr>
                  <tr>
                    [% IF empty || host_stats.passive_checks_disabled == 0 %]
                    <td width="100%" class='featureItemEnabledPassiveHostChecks'>All Hosts Enabled</td>
                    [% ELSE %]
                    <td width="100%" class='featureItemDisabledPassiveHostChecks'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=16384'>[% host_stats.passive_checks_disabled %] Host Disabled</a></td>
                    [% END %]
                  </tr>
                </table>
              </td>
              [% END %]
            </tr>
          </table>
        </td>
      </tr>
    </table>
    </div>
    </div>
[% PROCESS _footer.tt %]
