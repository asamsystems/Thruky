    <div class="panel panel-primary">
      <div class="panel-heading">
              <h5 class="panel-title text-center">
              [% IF service != '' and host != 'all' %]
              Service '[% service | html %]' On Host '[% host | html %]'
              [% ELSIF host != 'all' %]
              Host '[% host | html %]'
              [% ELSE %]
              All Hosts and Services
              [% END %]
              </h5>
              </div>
    <div class="panel-body">
    
[% PROCESS _logs.tt %]
    </div>
    </div>


